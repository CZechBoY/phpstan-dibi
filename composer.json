{
	"name": "phpstan/phpstan-dibi",
	"description": "Dibi class reflection extension for PHPStan",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "~7.0",
		"phpstan/phpstan": "^0.6",
		"dibi/dibi": "~3.0"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.12.0",
		"phing/phing": "^2.13.0",
		"phpunit/phpunit": "^5.3",
		"satooshi/php-coveralls": "^1.0",
		"slevomat/coding-standard": "dev-php7#961090ac32"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	}
}
